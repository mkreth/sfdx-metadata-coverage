{
  "name": "@mkreth/sfdx-metadata-coverage",
  "description": "A Salesforce CLI plugin to report the metadata coverage for a project",
  "version": "0.4.0",
  "author": "Magnus Kreth <magnus.kreth@gmx.de>",
  "bugs": "https://github.com/mkreth/sfdx-metadata-coverage/issues",
  "dependencies": {
    "@oclif/config": "^1",
    "@salesforce/command": "^4.2.0",
    "@salesforce/core": "^2.31.0",
    "@types/node-fetch": "^2.5.10",
    "@types/recursive-readdir": "2.2.0",
    "@types/xml2js": "^0.4.9",
    "node-fetch": "^2.6.1",
    "recursive-readdir": "2.2.2",
    "tslib": "^2",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/plugin-command-snapshot": "^2.2.2",
    "@salesforce/cli-plugins-testkit": "^1.4.24",
    "@salesforce/dev-config": "^2.1.0",
    "@salesforce/dev-scripts": "^1.0.2",
    "@salesforce/plugin-command-reference": "^1.4.7",
    "@salesforce/prettier-config": "^0.0.2",
    "@salesforce/ts-sinon": "1.3.21",
    "@types/chai-as-promised": "^7.1.4",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-salesforce": "^0.1.0",
    "eslint-config-salesforce-license": "^0.1.0",
    "eslint-config-salesforce-typescript": "^0.2.8",
    "eslint-plugin-header": "^3.0.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsdoc": "^37.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.2",
    "mocha": "^9.1.3",
    "nyc": "^15.1.0",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.2",
    "shx": "0.3.3",
    "sinon": "^12.0.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "/lib",
    "/messages",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/mkreth/sfdx-metadata-coverage",
  "keywords": [
    "force",
    "salesforce",
    "sfdx",
    "salesforcedx",
    "sfdx-plugin",
    "metadata",
    "coverage"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sfdx",
    "devPlugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-command-snapshot",
      "@salesforce/plugin-command-reference"
    ],
    "topics": {
      "mdcoverage": {
        "description": "Commands related to metadata coverage for files in a project"
      }
    }
  },
  "repository": "mkreth/sfdx-metadata-coverage",
  "scripts": {
    "build": "sf-build",
    "clean": "sf-clean",
    "clean-all": "sf-clean all",
    "clean:lib": "shx rm -rf lib && shx rm -rf coverage && shx rm -rf .nyc_output && shx rm -f oclif.manifest.json",
    "compile": "sf-compile",
    "docs": "sf-docs",
    "format": "sf-format",
    "lint": "sf-lint",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "sf-prepack",
    "prepublish": "sf-prepack",
    "prepare": "sf-prepack",
    "pretest": "sf-compile-test",
    "test": "sf-test",
    "test:command-reference": "./bin/run commandreference:generate --erroronwarnings",
    "test:deprecation-policy": "./bin/run snapshot:compare",
    "test:nuts": "nyc mocha \"**/*.nut.ts\" --slow 4500 --timeout 600000 --parallel",
    "version": "oclif-dev readme"
  },
  "husky": {
    "hooks": {
      "pre-commit": "sf-husky-pre-commit",
      "commit-msg": "sf-husky-commit-msg",
      "pre-push": "sf-husky-pre-push"
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
